{
  "name": "school-management-system-backend",
  "version": "1.0.0",
  "description": "School Management System Backend - Optimized for Nepal Education System",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --exec ts-node --transpile-only -r tsconfig-paths/register src/server.ts",
    "build": "node build.js",
    "build:strict": "tsc",
    "start": "node dist/server.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "typecheck": "tsc --noEmit",
    "migrate:up": "ts-node src/scripts/run-migrations.ts up",
    "migrate:down": "ts-node src/scripts/run-migrations.ts down",
    "seed": "ts-node -r tsconfig-paths/register src/scripts/seed-database.ts",
    "seed:staff": "ts-node -r tsconfig-paths/register src/scripts/seed-staff.ts",
    "seed:holidays": "ts-node -r tsconfig-paths/register src/scripts/seed-nepal-holidays.ts",
    "seed:academic": "ts-node -r tsconfig-paths/register src/scripts/seed-academic-module.ts",
    "seed:examinations": "ts-node -r tsconfig-paths/register src/scripts/seed-examinations.ts",
    "optimize:db": "ts-node src/scripts/optimize-database.ts",
    "update:admin-password": "ts-node -r tsconfig-paths/register src/scripts/update-admin-password.ts",
    "reset:admin-direct": "ts-node -r tsconfig-paths/register src/scripts/reset-admin-direct.ts",
    "seed:roles": "ts-node -r tsconfig-paths/register src/scripts/seed-roles-permissions.ts"
  },
  "keywords": [
    "school-management",
    "education",
    "nepal",
    "neb",
    "student-management"
  ],
  "author": "School Management System Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "connect-redis": "^7.1.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.17.3",
    "express-validator": "^7.0.1",
    "fastest-levenshtein": "^1.0.16",
    "fuse.js": "^7.1.0",
    "fuzzball": "^2.2.3",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "isomorphic-dompurify": "^2.35.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.5",
    "nepali-date-converter": "^3.1.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.7",
    "pdfkit": "^0.13.0",
    "qrcode": "^1.5.3",
    "rate-limit-redis": "^4.3.1",
    "redis": "^4.6.11",
    "sanitize-html": "^2.11.0",
    "sequelize": "^6.35.2",
    "sharp": "^0.33.1",
    "socket.io": "^4.6.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "xlsx": "^0.18.5",
    "xss": "^1.0.14"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/hpp": "^0.2.7",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.5",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.14",
    "@types/pdfkit": "^0.13.9",
    "@types/qrcode": "^1.5.5",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "eslint-plugin-security": "^2.1.0",
    "fast-check": "^4.5.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "sequelize-cli": "^6.6.2",
    "sqlite3": "^5.1.7",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
